#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_gh-pages@1.2.0@gh-pages/bin/gh-pages.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_gh-pages@1.2.0@gh-pages/bin/gh-pages.js" "$@"
  ret=$?
fi
exit $ret
